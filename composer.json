{
    "name": "laravel/laravel",
    "type": "project",
    "description": "The skeleton application for the Laravel framework.",
    "keywords": ["laravel", "framework"],
    "license": "MIT",
    "require": {
        "php": "^8.1",
        "algolia/algoliasearch-client-php": "^3.3",
        "datomatic/nova-enum-field": "^1.6",
        "guzzlehttp/guzzle": "^7.2",
        "inertiajs/inertia-laravel": "^0.6.3",
        "inspheric/nova-indicator-field": "^1.43",
        "kra8/laravel-snowflake": "^2.2",
        "laravel/framework": "^10.10",
        "laravel/horizon": "^5.20",
        "laravel/nova": "^4.0",
        "laravel/sanctum": "^3.2",
        "laravel/scout": "^10.2",
        "laravel/tinker": "^2.8",
        "laravel/vapor-core": "^2.31",
        "laravel/vapor-ui": "^1.7",
        "league/flysystem-aws-s3-v3": "^3.15",
        "livewire/livewire": "^2.12",
        "muxinc/mux-php": "^3.11",
        "nrml-co/nova-big-filter": "^1.0",
        "outl1ne/nova-sortable": "^3.4",
        "phpfastcache/phpfastcache": "^9.1",
        "predis/predis": "^2.2",
        "prinsfrank/standards": "^2.1",
        "raiym/instagram-php-scraper": "^0.12.9",
        "spatie/eloquent-sortable": "^4.0",
        "spatie/enum": "^3.13",
        "spatie/laravel-data": "^3.6",
        "spatie/laravel-event-sourcing": "^7.3",
        "spatie/laravel-model-states": "^2.4",
        "spatie/laravel-onboard": "^2.4",
        "spatie/laravel-queueable-action": "^2.14",
        "spatie/laravel-ray": "^1.33",
        "spatie/laravel-tags": "^4.4",
        "spatie/laravel-typescript-transformer": "^2.3",
        "spatie/laravel-view-models": "^1.5",
        "spatie/nova-tags-field": "^4.0",
        "spatie/typescript-transformer": "^2.2",
        "tightenco/ziggy": "^1.0",
        "vyuldashev/nova-money-field": "^2.0"
    },
    "require-dev": {
        "doctrine/dbal": "^3.7",
        "fakerphp/faker": "^1.9.1",
        "laravel/breeze": "^1.21",
        "laravel/dusk": "^7.11",
        "laravel/pint": "^1.0",
        "laravel/sail": "^1.18",
        "mockery/mockery": "^1.4.4",
        "nunomaduro/collision": "^7.0",
        "pestphp/pest": "^2.15",
        "pestphp/pest-plugin-drift": "^2.3",
        "pestphp/pest-plugin-faker": "^2.0",
        "pestphp/pest-plugin-laravel": "^2.2",
        "phpunit/phpunit": "^10.1",
        "spatie/laravel-ignition": "^2.0"
    },
    "autoload": {
        "psr-4": {
            "App\\": "src/App/",
            "Domain\\": "src/Domains/",
            "Support\\": "src/Support/",
            "Database\\Factories\\": "database/factories/",
            "Database\\Seeders\\": "database/seeders/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Tests\\": "tests/"
        }
    },
    "scripts": {
        "post-autoload-dump": [
            "Illuminate\\Foundation\\ComposerScripts::postAutoloadDump",
            "@php artisan package:discover --ansi"
        ],
        "post-update-cmd": [
            "@php artisan vendor:publish --tag=laravel-assets --ansi --force"
        ],
        "post-root-package-install": [
            "@php -r \"file_exists('.env') || copy('.env.example', '.env');\""
        ],
        "post-create-project-cmd": [
            "@php artisan key:generate --ansi"
        ]
    },
    "extra": {
        "laravel": {
            "dont-discover": []
        }
    },
    "config": {
        "optimize-autoloader": true,
        "preferred-install": "dist",
        "sort-packages": true,
        "allow-plugins": {
            "pestphp/pest-plugin": true,
            "php-http/discovery": true
        }
    },
    "repositories": [
        {
            "type": "composer",
            "url": "https://nova.laravel.com"
        }
    ],
    "minimum-stability": "stable",
    "prefer-stable": true
}
